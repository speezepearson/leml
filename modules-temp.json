{
	"math": {
		"delimiter": "$",
		"create-script": "echo '<img style=\"max-height:1.5ex\" src=\"'`typeset-math`'\" />'"
	},
	
	"identity": {
		"delimiter": "#",
		"create": "lambda fmt, s: s"
	},

	"code": {
		"delimiter": "`",
		"create": "lambda fmt, s: '<code>{}</code>'.format(s)"
	},

	"formatty": {
		"delimiter": "@",
		"create": "lambda fmt, s: '[{fmt}]{s}[/{fmt}]'.format(fmt=fmt, s=s)"
	}
}

